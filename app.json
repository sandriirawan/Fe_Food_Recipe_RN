{
  "expo": {
    "name": "Food Recipe",
    "slug": "food-recipe",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/FoodRecipe.png",
    "splash": {
      "image": "./assets/FoodRecipe.png",
      "resizeMode": "contain",
      "backgroundColor": "#EFC81A"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.sandriirawan.reactnativenativebase"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/FoodRecipe.png",
        "backgroundColor": "#EFC81A"
      },
      "package": "com.sandriirawan.reactnativenativebase",
      "permissions": ["CAMERA", "CAMERA_ROLL"] 
    },
    "scheme": "acme",
    "web": {
      "bundler": "metro"
    },
    "platforms": ["ios", "android", "web"],
    "extra": {
      "eas": {
        "projectId": "6dc2428b-3e2b-4e37-ae51-1e9b0fcd523d"
      }
    }
  }
}
